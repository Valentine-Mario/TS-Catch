{
  "name": "ts-catch",
  "version": "1.0.0",
  "description": "A typescript linter that helps increase performance and possibly catch things that could lead to runtime errors",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "ts-node src/index.ts src/test",
    "all": "TESTNAME=src/test ts-node src/index &&TESTNAME=src/test ts-node src/lib/check.ts",
    "lint": "TESTNAME=$npm_config_path ts-node src/index.ts &&TESTNAME=$npm_config_path ts-node src/lib/check.ts",
    "bless": "ts-node src/lib/bless.ts"
  },
  "author": "Valentine Oragbakosi",
  "license": "ISC",
  "dependencies": {
    "diff": "^5.0.0",
    "glob": "^7.1.7",
    "ts-morph": "^11.0.0",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@types/diff": "^5.0.0",
    "@types/glob": "^7.1.3",
    "@types/node": "^15.12.2"
  }
}
